.title{
    height: 50rpx;
    font-size: 12px;
    color: #333;
    background: #f2f2f2;
    line-height: 50rpx;
    padding: 0rpx 30rpx;
    border-top: 1rpx solid #ddd;
    border-bottom: 1rpx solid #ddd;
}
.reason{
    height: 88rpx;
    line-height: 88rpx;
    border-bottom: 1rpx solid #f2f2f2;
}
.picker{
    margin-left: 20rpx;
}
.circle{
    height: 44rpx;
    width: 44rpx;
    border: 1rpx solid #ddd;
    border-radius: 100%;
    margin-right: 30rpx;
}
.circleC{
    height: 44rpx;
    width: 44rpx;
    border-radius: 100%;
    margin-right: 30rpx;
    background: #ff2e6b;
}
.button{
    height: 100rpx;
    background: #ff2e6b;
    line-height: 100rpx;
    text-align: center;
    position: fixed;
    bottom: 0rpx;
    width: 100%;
    color: white;
}
.otherBox{
    position: absolute;
    top: 50%;
    margin-top: -129rpx;
    height: 258rpx;
    width: 80%;
    left: 7.5%;
    border: 1rpx solid #ddd;
    padding: 20rpx;
    background: white;
}
.textarea{
    border: 1rpx solid #f2f2f2;
    height: 120rpx;
    margin-top: 20rpx;
}
.reasonButton{
    height: 80rpx;
    background: #f2f2f2;
    color: #666;
    position: absolute;
    bottom: 0rpx;
    line-height: 80rpx;
    width: 100%;
    left: 0%;
    text-align: center;
}